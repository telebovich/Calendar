@page
@model Calendar.Pages.SelectYearModel
@{
    ViewData["Title"] = "SelectYear";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>@Model.DecadeBeginYear - @Model.DecadeEndYear</h1>

<table class="table">
    @for (int i = 0; i < 4; i++)
    {
        <tr>
            @for (int j = 0; j < 4; j++)
            {
                <td>
                    @if (Model.YearsArray[i, j] > 0)
                    {
                        <div>
                            <a class="text-dark" href="/SelectMonth/@Model.YearsArray[i, j]">@Model.YearsArray[i, j]</a>
                        </div>
                    }
                </td>
            }
        </tr>
    }
</table>

